{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "rating": {
      "type": "integer"
    },
    "oldPrice": {
      "type": "integer"
    },
    "newPrice": {
      "type": "integer"
    },
    "discount": {
      "type": "decimal"
    },
    "buttontext": {
      "type": "string"
    },
    "discribtion": {
      "type": "text"
    },
    "discounted": {
      "type": "boolean"
    },
    "remainingTime": {
      "type": "boolean"
    },
    "rationgfill": {
      "type": "integer"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "in_stoct": {
      "type": "boolean"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "burger",
        "chicken",
        "colddrinks",
        "mexican",
        "pizza"
      ]
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "s",
        "m",
        "l",
        "r"
      ]
    },
    "brand": {
      "type": "enumeration",
      "enum": [
        "burgerking",
        "kfc",
        "donion's",
        "mcdonald's",
        "pizzahut",
        "tacobell"
      ]
    }
  }
}
